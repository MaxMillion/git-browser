{
  "name": "Git-Browser",
  "version": "0.1.3",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "Browse Git Repos offline.",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    {"socket": [ "tcp-connect:*:*" ]}
  ]
}
